<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpaper"
    android:id="@+id/HomeLayout"
    tools:context=".vHome">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="95dp"
            android:layout_marginTop="48dp"
            android:text="Welcom "
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewName"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="152dp"
            android:text="Name "
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewch"
            android:layout_width="101dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="42dp"
            android:text="Cridit Hours"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView11" />

        <TextView
            android:id="@+id/textViewch2"
            android:layout_width="101dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="84dp"
            android:text="My Badges"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewch" />

        <Button
            android:id="@+id/coloring"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="42dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/coloring"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.968"
            app:layout_constraintStart_toEndOf="@+id/textViewch"
            app:layout_constraintTop_toBottomOf="@+id/textViewName" />

        <View
            android:id="@+id/divider"
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="8dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewch" />

        <ImageView
            android:id="@+id/level1"
            android:layout_width="85dp"
            android:layout_height="84dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="80dp"
            android:tint="@color/colorPrimaryDark"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewch2"
            app:srcCompat="@drawable/level1" />

        <ImageView
            android:id="@+id/level5"
            android:tint="@color/colorPrimaryDark"
            android:layout_width="85dp"
            android:layout_height="84dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="45dp"
            app:layout_constraintStart_toEndOf="@+id/level4"
            app:layout_constraintTop_toBottomOf="@+id/level2"
            app:srcCompat="@drawable/level8" />

        <ImageView
            android:id="@+id/level4"
            android:tint="@color/colorPrimaryDark"
            android:layout_width="85dp"
            android:layout_height="84dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="45dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/level1"
            app:srcCompat="@drawable/level6" />

        <ImageView
            android:id="@+id/level6"
            android:layout_width="85dp"
            android:layout_height="84dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="44dp"
            android:tint="@color/colorPrimaryDark"
            app:layout_constraintStart_toEndOf="@+id/level5"
            app:layout_constraintTop_toBottomOf="@+id/level3"
            app:srcCompat="@drawable/level9" />

        <ImageView
            android:id="@+id/level2"
            android:layout_width="85dp"
            android:layout_height="83dp"
            android:layout_marginStart="60dp"
            android:tint="@color/colorPrimaryDark"
            android:layout_marginTop="80dp"
            app:layout_constraintStart_toEndOf="@+id/level1"
            app:layout_constraintTop_toBottomOf="@+id/textViewch2"
            app:srcCompat="@drawable/level2" />

        <ImageView
            android:id="@+id/level3"
            android:tint="@color/colorPrimaryDark"

            android:layout_width="85dp"
            android:layout_height="84dp"
            android:layout_marginStart="59dp"
            android:layout_marginTop="80dp"
            app:layout_constraintStart_toEndOf="@+id/level2"
            app:layout_constraintTop_toBottomOf="@+id/textViewch2"
            app:srcCompat="@drawable/level4" />

    </android.support.constraint.ConstraintLayout>

    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <android.support.design.widget.BottomNavigationView
            android:id="@+id/navigation"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_gravity="bottom"
            app:menu="@menu/menu_vnavigation" />


    </android.support.design.widget.CoordinatorLayout>
</RelativeLayout>